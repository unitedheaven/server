{
  "name": "united-heaven-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "start": "npm run build:ts && node dist/app.js",
    "build:ts": "tsc",
    "lint-src": "eslint --ignore-path .eslintignore --ext .ts src",
    "seed:SDG": "ts-node -r tsconfig-paths/register src/scripts/seedSDGs.ts",
    "seed:numberToId": "ts-node -r tsconfig-paths/register src/scripts/sdgNumberToId.ts",
    "seed:charity": "ts-node -r tsconfig-paths/register src/scripts/seedCharities.ts",
    "seed:news": "ts-node -r tsconfig-paths/register src/scripts/seedNews.ts",
    "seed:events": "ts-node -r tsconfig-paths/register src/scripts/seedEvents.ts",
    "seed:all": "pnpm seed:SDG && pnpm seed:numberToId && pnpm seed:charity",
    "aws:build": "pnpm build:ts && sam build && sam deploy --guided"
  },
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@types/node": "^20.8.8",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "csv-parser": "^3.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/aws-lambda": "^3.4.2",
    "@fastify/cors": "^8.4.1",
    "@fastify/multipart": "^8.0.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
    "mongoose": "^8.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  }
}
